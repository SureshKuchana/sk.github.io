    "next-themes": "^0.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
